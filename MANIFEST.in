
# Exclude all .nix and .yaml files at the root and in any subdirectory
exclude *.nix
exclude *.yaml
exclude *.lock
exclude .envrc
exclude pytest.ini
exclude .coveragerc
recursive-exclude * *.nix
recursive-exclude * *.yaml
recursive-exclude * *.lock
recursive-exclude * .envrc
recursive-exclude * pytest.ini
recursive-exclude * .coveragerc


